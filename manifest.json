{
    "manifest_version": 3,
    "name": "Teams Green",
    "version": "1.1.2",
    "description": "A Chrome extension that keeps you available on teams unless the screensaver is active.",
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "src/images/teamsIcon.png",
        "128": "src/images/teamsIcon.png"
    },
    "action": {
        "default_popup": "src/html/settings.html"
    },
    "permissions": [
        "storage",
        "alarms",
        "tabs",
        "scripting"
    ],
    "host_permissions": [
        "https://*.teams.microsoft.com/*"
    ]
}
